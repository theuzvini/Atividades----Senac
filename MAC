		public void Three() {
			System.out.println("------------------------------------");
			for (int i = 1; i <= 20; i++) {
				System.out.println("Eu, Davi gosto de estudar Algoritmos!");
			}
			System.out.println("------------------------------------");
			System.out.println("");
			System.out.println("Codigo:");
			System.out.println("");
			System.out.println("for (int i = 1; i <= 20; i++) {");
			System.out.println("	System.out.println(\"Eu, Davi gosto de estudar Algoritmos!\");");
			System.out.println("}");
		}
		
