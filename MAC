package Java;
import java.util.Scanner;
public class Task1 {
Scanner mat = new Scanner (System.in);

	public void Menu() {
		int valor;
		System.out.println("Digite a Questão que deseja saber: [1] [2] [3] [4] [5] [6] [7] [8]");
		valor = mat.nextInt();
		if (valor == 1) {
			System.out.println("Bem-Vindo a Questão 1");
			System.out.println("");
			One();
		}
		if (valor == 2) {
			System.out.println("Bem-Vindo a Questão 2");
			System.out.println("");
			Two();
		}
		if (valor == 3) {
			System.out.println("Bem-Vindo a Questão 3");
			System.out.println("");
			Three();
		}
		if (valor == 4) {
			System.out.println("Bem-Vindo a Questão 4");
			System.out.println("");
			For();
		}
		if (valor == 5) {
			System.out.println("Bem-Vindo a Questão 5");
			System.out.println("");
			Five();
		}
		if (valor == 6) {
			System.out.println("Bem-Vindo a Questão 6");
			System.out.println("");
			Six();
		}
		if (valor == 7) {
			System.out.println("Bem-Vindo a Questão 7");
			System.out.println("");
			Seven();
		}
		if (valor == 8) {
			System.out.println("Bem-Vindo a Questão 8");
			System.out.println("");
			Height();
		}
		if (valor > 8) {
			System.out.println("Essa Questão Não Existe!");
			System.out.println("");
			Menu();
		}
	}
	
