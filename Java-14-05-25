package Pacote;
import java.util.Scanner;

public class Produtos {
Scanner text = new Scanner (System.in);
int Logi;
int quant;
double valor;
double valortot;
String[] item;

	public void Itens() {
		System.out.println("");
		System.out.println("");
		System.out.println("");
		System.out.println("");
		System.out.println("---------------------");
		System.out.println("Preço Baixo é aqui!");
		System.out.println("---------------------");
		System.out.print("Ver:");
		System.out.println("[1] Utensilios [2] Panelas [3] Formas");
		Logi = text.nextInt();
		if (Logi == 1) {
			Utensilios();
		}
		if (Logi == 2) {
			Panelas();
		}
		if (Logi == 3) {
			Formas();
		}
		if (Logi != 1 & Logi != 2 & Logi != 3) {
			Itens();
		}
	}
	
	public void Utensilios() {
