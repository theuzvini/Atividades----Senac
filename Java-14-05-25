		System.out.println("-------------------");
		System.out.println("Insira o código do produto que deseja ver:");
		Logi = text.nextInt();
		switch (Logi) {
		case 1: {
			quant = 1;
			valor = 21.00; 
			valortot = valor * quant;
			item[0] = "Espátula de silicone";
			System.out.println("");
