Login
------------------------------------------------------------------------------------------------------------------------------------------------------

package Pacote;
import java.util.Scanner;

public class Login {
Scanner text = new Scanner (System.in);
String Name;
String Email;
String Pass;
String Logi;

	public void Inicial() {
		int a;
		System.out.println("Olá! Bem-Vindo ao Mix Mateus");
		System.out.println("");
		System.out.println("------------------------------------");
		System.out.println("VOCÊ JÁ POSSUI CONTA?");
		System.out.println("Insira [1] para CADASTRAR");
		System.out.println("Insira [2] para LOGAR");
		System.out.println("_____");
		a = text.nextInt();
		System.out.println("------------------------------------");
		if (a == 1) {
			Cadastrar();
		}
		if (a == 2) {
			Logar();
		}
		if (a != 1 & a != 2) {
			Inicial();
		}
	}
	
	private void Cadastrar() {
		System.out.println("");
		System.out.println("");
		System.out.println("Olá! Bem-Vindo ao Mix Mateus");
		System.out.println("");
		System.out.println("------------------------------------");
		System.out.println("CADASTRAR CONTA");
		System.out.println("Insira seu NOME COMPLETO");
		Name = text.next();
		System.out.println("Insira um EMAIL");
		Email = text.next();
		System.out.println("Insira uma SENHA");
		Pass = text.next();
		System.out.println("------------------------------------");
		System.out.println("INSERIR [Sim] [Não]");
		Logi = text.next();
		switch (Logi) {
		case "Sim": {
			Menu();
		}
		case "Não": {
			Inicial();
		}
		}
	}
	
	private void Logar() {
		System.out.println("");
		System.out.println("");
		System.out.println("Olá! Bem-Vindo ao Mix Mateus");
		System.out.println("");
		System.out.println("------------------------------------");
		System.out.println("LOGAR CONTA");
		System.out.println("Insira seu EMAIL");
		
		System.out.println("Insira sua SENHA");
		
		System.out.println("------------------------------------");
		System.out.println("INSERIR [Sim] [Não]");
		Logi = text.next();
		switch (Logi) {
		case "Sim": {
			Menu();
		}
		case "Não": {
			Inicial();
		}
		}
	}
	
	private void Menu() {
		System.out.println("");
		System.out.println("");
		System.out.println("Olá! Bem-Vindo ao Mix Mateus");
		System.out.println("");
		System.out.println("------------------------------------");
		System.out.println("Insira [1] para ver Produtos");
		System.out.println("Insira [2] para ver Carrinho");
		System.out.println("Insira [3] para ver Suporte");
		System.out.println("Insira [4] para ver Pagamentos");
		System.out.println("------------------------------------");
		System.out.println("O que deseja?");
		Logi = text.next();
		switch (Logi) {
		case "1": {
			Produtos log = new Produtos();
			log.Itens();
		}
		case "2": {
			
		}
		case "3": {
			
		}
		case "4": {
			
		}
		}
	}
}

